{
  "name": "project-b",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/project-b",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/next:build",
      "outputs": ["{workspaceRoot}/dist/apps/project-b"],
      "options": {
        "outputPath": "dist/apps/project-b"
      }
    },
    "serve": {
      "executor": "@nx/next:server",
      "options": {
        "buildTarget": "project-b:build",
        "dev": true,
        "port": 3001
      }
    },
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "next dev -p 3001",
        "cwd": "apps/project-b"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "options": {
        "lintFilePatterns": ["apps/project-b/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/apps/project-b"],
      "options": {
        "jestConfig": "apps/project-b/jest.config.js"
      }
    }
  }
}
